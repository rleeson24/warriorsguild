@model ErrorViewModel
@{
    ViewData["Title"] = "Error";
    Layout = null;
}

@using Microsoft.AspNetCore.Hosting
@using Microsoft.Extensions.Hosting
@using WarriorsGuild.Utilities
@inject IWebHostEnvironment env


<!DOCTYPE html>
<html lang="en">
